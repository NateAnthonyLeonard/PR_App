# platform :ios, '12.0'

target 'PRapp' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for PRapp
  # add the Firebase pod for Google Analytics
    pod 'Firebase/Analytics','7.4-M1'
  # add pods for any other desired Firebase products
  # https://firebase.google.com/docs/ios/setup#available-pods
    pod 'Firebase/Core','7.4-M1'
    pod 'Firebase/Auth','7.4-M1'
    pod 'Firebase/Storage','7.4-M1'
    pod 'Firebase/Database','7.4-M1'
    pod 'Firebase/Firestore','7.4-M1'

  target 'PRappTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'PRappUITests' do
    # Pods for testing
  end

end